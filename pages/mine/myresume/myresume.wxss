/**myresume.wxss**/
.group{
  display: block;
  width: 100%;
}
.group-header{
  line-height: 70rpx;
  display: flex;
  padding: 0 20rpx;
  background: #f9f9f9;
}
.group-header::before{
  content: '';
	display: inline-block;
	vertical-align: -6rpx;
	width: 10rpx;
	height: 70rpx;
	background: #07fb42;
	margin-right: 8rpx;
}
.group-body{
  background: #fff;
  border-top: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
}

.btn-submit{
  padding: 20rpx;
}
.btn-green{
  background: #a6eafd;
  color: #fff;
}